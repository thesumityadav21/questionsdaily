MY SOLUTION -


#include<iostream>
#include<algorithm>
using namespace std;

int main()
{
    int t;
cin>>t;

while(t--){
  int n,a,b,c;
  cin>>n>>a>>b>>c;
  
  if(n <= b && a+c >= n ){
      cout<<"YES";
  }
  else{
      cout<<"NO";
  }
}
return 0;
}
